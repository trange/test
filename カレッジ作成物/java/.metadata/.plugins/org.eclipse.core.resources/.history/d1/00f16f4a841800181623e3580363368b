public class Person {

	private String name = null;
	private int age = 0;

	public Person(String n, int i) {
		// TODO 自動生成されたコンストラクター・スタブ
	}
	public String getName(){
		return name;
	}
	public void setName(String name){
		this.name = name;
	}
	public int getAge() {
