
public class Test {

	public static void main(String[] args) {
		// Java練習
		int number=88;
		if(number<100){
			if(number<20){
				System.out.println("未成年");
			}else if(number==77){
				System.out.println("喜寿");
			}else if(number==88){
				System.out.println("米寿");
			}else{
				System.out.println("成人");
			}
		}
	}

}
